<?xml version="1.0" encoding="utf-8"?>
<Container version="2">
  <Beta>False</Beta>
  <Category></Category>
  <Date>2020-12-18</Date>
  <Name>Neos-Dev</Name>
  <Description>This is a Docker Container for Neos in Dev configuration
  </Description>
  <Overview>This is a custom template made for an own unraid server</Overview>
  <Support></Support>
  <Registry>https://gitlab.winterkamp.eu/Luca/neos-docker-development-environment/</Registry>
  <GitHub>https://gitlab.winterkamp.eu/Luca/neos-docker-development-environment</GitHub>
  <Repository>registry.winterkamp.eu/luca/neos-docker-development-environment</Repository>
  <ExtraParams>-p 80:8081</ExtraParams>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/neos-dev</HostDir>
      <ContainerDir>/app</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment/>
  <WebUI>[IP]:[PORT]</WebUI>
  <Banner></Banner>
  <Icon></Icon>
  <Network Default="bridge">br0</Network>
  <Config Type="Path" Name="Storage Path" Target="/app" Default="/mnt/user/appdata/neos-dev" Mode="rw" Display="always-hide" Required="true" Description="This is the storagepath for Neos">/mnt/user/appdata/neos-dev</Config>
</Container>
